program FlairB (myInt : integer,
				myBool: boolean);

		function timesTen (): integer
		begin
			return myInt * 10
		end;
		function divideTen (): integer
		begin
			return myInt / 10
		end;
		function addTen (): integer
		begin
			return myInt + 10
		end;
		function subtractTen (): integer
		begin
			return myInt - 10
		end;
		function lessthanTen (): boolean
		begin
			return myInt < 10
		end;
		function andBool (): boolean
		begin
			return myBool and lessthanTen()
		end;
		function orBool (): boolean
		begin
			return myBool or lessthanTen()
		end;
		function whatsBool (): boolean
		begin
			return myBool
		end;
		function whatsInt (): integer
		begin
			return myInt
		end;
		begin
			print(timesTen());
			print(divideTen());
			print(addTen());
			print(subtractTen());
			print(lessthanTen());
			print(andBool());
			print(orBool());
			print(whatsBool());
			return(whatsInt())
		end;
