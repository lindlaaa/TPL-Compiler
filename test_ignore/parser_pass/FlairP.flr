program FlairP (myInt : integer, myBool : boolean);

		function timesTenP (): integer
		begin
			return(myInt * 10)
		end;
		function divideTenP (): integer
		begin
			return(myInt / 10)
		end;
		function addTenP (): integer
		begin
			return(myInt + 10)
		end;
		function subtractTenP (): integer
		begin
			return(myInt - 10)
		end;
		function lessthanTenP (): boolean
		begin
			return(myInt < 10)
		end;
		function andBoolP (): boolean
		begin
			return(myBool and lessthanTen())
		end;
		function orBoolP (): boolean
		begin
			return(myBool or lessthanTen())
		end;
		function whatsBoolP (): boolean
		begin
			return(myBool)
		end;
		function whatsIntP (): integer
		begin
			return(myInt)
		end;
		begin
			print(timesTenP());
			print(divideTenP());
			print(addTenP());
			print(subtractTenP());
			print(lessthanTenP());
			print(andBoolP());
			print(orBoolP());
			print(whatsBoolP());
			return(whatsIntP())
		end;
